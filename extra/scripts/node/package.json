{
  "name": "dotfiles",
  "version": "1.0.0",
  "description": "elentok's dotfiles",
  "scripts": {
    "format": "prettier --write '**/*.js' '**/*.ts' '**/*.css'",
    "format:lua": "stylua --glob '**/*.lua' --config-path=$DOTF/stylua.toml .",
    "ts:build": "tsc",
    "ts:watch": "tsc --watch",
    "start": "npm run ts:watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elentok/dotfiles.git"
  },
  "author": "David Elentok",
  "license": "UNLICENSED",
  "homepage": "https://github.com/elentok/dotfiles#readme",
  "private": true,
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "commander": "^9.4.0",
    "kdbxweb": "^2.1.1",
    "prettier": "^2.7.1",
    "prompt": "^1.3.0",
    "source-map-support": "^0.5.21",
    "typescript": "^4.7.4",
    "underscore": "^1.13.4"
  },
  "devDependencies": {
    "@types/node": "^18.7.11",
    "@types/serve-static": "^1.15.0",
    "@types/underscore": "^1.11.4"
  },
  "packageManager": "yarn@3.5.1"
