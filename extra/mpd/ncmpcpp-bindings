def_key "d"
  delete_playlist_items

def_key "d"
  delete_browser_items

def_key "d"
  delete_stored_playlist

def_key "p"
  pause

def_key "k"
  scroll_up

def_key "K"
  move_selected_items_up

def_key "j"
  scroll_down

def_key "J"
  move_selected_items_down

def_key "l"
  next_column
  volume_down

def_key "h"
  previous_column
  volume_up

def_key "H"
  show_help

def_key "="
  volume_up
