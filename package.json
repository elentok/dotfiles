    "@typescript-eslint/parser": "^5.34.0",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^9.4.0",
