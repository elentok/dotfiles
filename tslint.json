{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "semicolon": [true, "never"],
    "quotemark": [true, "single"],
    "no-console": [true, "log"],
    "arrow-parens": [false],
    "trailing-comma": [true, { "multiline": "never", "singleline": "never" }]
  },
  "rulesDirectory": []
}
