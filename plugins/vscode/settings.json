{
    "window.zoomLevel": 1,
    "terminal.integrated.lineHeight": 1.1,
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
    "editor.rulers": [
        80,
        100
    ],
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "terminal.integrated.shell.windows": "C:\\Windows\\System32\\bash.exe",
    "cSpell.userWords": [
        "Ribcage"
    ],
    "[markdown]": {
        "editor.quickSuggestions": true
    }
}