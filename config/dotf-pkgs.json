{
  "$schema": "./dotf-pkgs.schema.json",
  "packages": [
    {
      "name": "neovim",
      "github_repo": "neovim/neovim",
      "version": "0.4.3",
      "platforms": {
        "linux": {
          "bin": "bin/nvim",
          "asset_regexp": "nvim-linux64.tar.gz"
        },
        "darwin": {
          "bin": "bin/nvim",
          "asset_regexp": "nvim-macos.tar.gz"
        }
      }
    },
    {
      "name": "exa",
      "github_repo": "ogham/exa",
      "version": "0.9.0",
      "platforms": {
        "linux": {
          "bin": "exa-linux-x86_64",
          "asset_regexp": "exa-linux-x86_64.*"
        },
        "darwin": {
          "bin": "exa-macos-x86_64",
          "asset_regexp": "exa-macos-x86_64.*"
        }
      }
    }
  ]
}
